{
	"name": "clean-house",
	"private": true,
	"sideEffects": false,

	"scripts": {
		"build": "run-s build:*",
		"build:remix": "remix build",
		"build:server": "esbuild --platform=node --format=cjs ./server.ts --bundle --outfile=build/server.cjs",
		"dev": "run-s build:server && run-p dev:build dev:remix",
		"dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
		"dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.cjs",
		"dev:remix": "remix dev -c 'node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.cjs'",
		"docker": "docker-compose up -d",
		"format": "prettier --write .",
		"format:repo": "npm run format && npm run lint:repo -- --fix",
		"lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
		"lint:repo": "npm run lint -- --config ./.eslintrc.repo.js",
		"setup": "prisma generate && prisma migrate deploy && prisma db seed",
		"start": "cross-env NODE_ENV=production node ./build/server.cjs",
		"start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.cjs",
		"test": "vitest",
		"test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
		"pretest:e2e:run": "npm run build",
		"test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
		"typecheck": "tsc && tsc -p cypress",
		"validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
	},
	"prettier": {},
	"eslintIgnore": [
		"/node_modules",
		"/build",
		"/public/build",
		"/postgres-data"
	],
	"dependencies": {
		"@conform-to/react": "^0.8.2",
		"@conform-to/zod": "^0.8.2",
		"@isaacs/express-prometheus-middleware": "^1.2.1",
		"@prisma/client": "^5.2.0",
		"@radix-ui/react-accordion": "^1.1.2",
